<table class="table table-hover">
  <tr>
    <th><%= t ".created_date" %></th>
    <th><%= t ".lesson" %></th>
    <th><%= t ".duration" %></th>
    <th><%= t ".spent_time" %></th>
    <th><%= t ".score" %></th>
    <th><%= t ".result" %></th>
  </tr>
  <% i = Settings.number.zero %>
  <% @lesson_logs.each do |ql| %>
    <% status = get_status(ql) %>
    <tr class="<%= status[1] %>">
      <td><%= ql.created_at.strftime Settings.time.format %></td>
      <td><%= link_to ql.lesson.name, url_for(ql) %></td>
      <td>#</td>
      <td><%= Time.at ql.spend_time %></td>
      <td><%= @results[i] %></td>
      <td><%= status[0] %></td>
    </tr>
  <% i += Settings.number.one %>
  <% end %>
</table>
